{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
   },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", { "code": 100, "tabWidth": 4, "comments": 200 }],
        "arrow-parens": ["error", "as-needed"],
        "comma-dangle": ["error", "never"]
   }
}
